{
  "name": "notalogger",
  "version": "1.0.9",
  "description": "A logger that is super extensible by middlewares. Does not log by default.",
  "keywords": [
    "logger",
    "middleware",
    "node",
    "nodejs",
    "typescript",
    "es6",
    "logging",
    "extend",
    "plugin"
  ],
  "main": "dist/logger.js",
  "types": "dist/types",
  "scripts": {
    "build_dev": "webpack -p",
    "build_prod": "webpack -p --config ./webpack.prod.config.js",
    "start": "npm run build_dev && node ./example",
    "test": "npm run lint && npm run jest",
    "jest": "jest",
    "lint": "eslint --ext .js,.ts ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpolack/nologger.git"
  },
  "author": "Jonah Polack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jpolack/nologger/issues"
  },
  "homepage": "https://github.com/jpolack/nologger#readme",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "clean-webpack-plugin": "^3.0.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-plugin-import": "^2.18.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^9.4.2",
    "typescript": "^3.5.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {}
}